   "source": [
    "# converting measurements to numerical variables \n",
    "# we have to change the columns 14, 15, 20-22 \"height\", \"weight\", \"value\", \"wage\", \"clause\"\n",
    "\n",
    "raw_data['height_cm'] = (raw_data['height']\n",
    " .str.extract(r'(\\d+)\\'\\s*(\\d+)\"')\n",
    " .astype(float)\n",
    " .mul([12*2.54, 2.54])\n",
    " .sum(axis=1)\n",
    " )\n",
    "\n",
    "raw_data.head()"
   ]
